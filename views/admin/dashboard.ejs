<% layout('layout') -%>

<h1>OnDemCom</h1>
<p class="lead">Welcome <%= user.profile.name || user.email %>.</p>
<hr/>
    <% if(!agentList.length) { %>
    	<p>No Agent Added</p>
    <% } else { %>
    	<table>
			<% for(var i=0; i < agentList.length; i++) { %>
			   <tr>
			     <td><%= agentList[i].email %></td>
			     <td><%= agentList[i].id %></td>
			   </tr>
			<% } %>
		</table>
    <% } %>
